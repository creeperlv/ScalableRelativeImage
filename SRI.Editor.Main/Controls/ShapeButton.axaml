<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="200" d:DesignHeight="50"
             x:Class="SRI.Editor.Main.Controls.ShapeButton">
  <Button Name="MainButton" HorizontalAlignment="Stretch">
    <Grid>
      <TextBlock Name="MainText" HorizontalAlignment="Left" Text="&lt;Shape&gt;"/>
      <TextBlock Name="SubText" HorizontalAlignment="Right" FontSize="12" Text="in Dll" Foreground="#A0A0A0A0"/>
    </Grid>
  </Button>
</UserControl>
